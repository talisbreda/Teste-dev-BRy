{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User",
  "description": "User data",
  "type": "object",
  "properties": {
    "users": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "name": {
            "type": "string",
            "minLength": 1
          },
          "photo": {
            "type": "string",
            "minLength": 1
          }
        },
        "required": [
          "id",
          "name",
          "photo"
        ]
      }
    }
  }
}